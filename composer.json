{
    "name": "dewsign/nova-blog",
    "description": "Blogging plugin for Laravel Nova",
    "type": "package",
    "keywords": [
        "laravel",
        "nova",
        "blog",
        "articles"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "m2de@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "maxfactor/support": "dev-master",
        "benjaminhirsch/nova-slug-field": "^1.0",
        "dewsign/nova-repeater-blocks": "dev-master",
        "epartment/nova-dependency-container": "^1.1",
        "silvanite/brandenburg": "^1.0",
        "dewsign/nova-field-sortable": "^0.2.0",
        "dewsign/laravel-authors": "^1.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Dewsign\\NovaBlog\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dewsign\\NovaBlog\\Providers\\PackageServiceProvider",
                "Dewsign\\NovaBlog\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nickfairchild/dewsign-maxfactor-laravel-support.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nickfairchild/dewsign-nova-repeater-blocks.git"
        }
    ]
}
